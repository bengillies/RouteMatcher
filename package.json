{
  "name": "route-matcher",
  "version": "0.1.0",
  "description": "Route matching library based on URLPattern, supporting child routes",
  "type": "module",
  "license": "BSD-3-Clause",
  "main": "./dist/route-matcher.js",
  "module": "./dist/route-matcher.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/route-matcher.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.2"
  },
  "scripts": {
    "dist": "vite build",
    "lint": "eslint . --ext .ts --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest run --browser",
    "test:watch": "vitest --browser",
    "test:types": "tsc --noEmit"
  },
  "keywords": [
    "routes",
    "matcher",
    "typescript",
    "browser"
  ],
  "author": "Ben Gillies",
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/debug": "^4.1.12",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitest/browser": "^4.0.7",
    "@vitest/browser-playwright": "^4.0.7",
    "@vitest/coverage-v8": "^4.0.7",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.2.1",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.7"
  }
}
